{
  "name": "ms-extended",
  "version": "0.1.5",
  "description": "Milliseconds utility library extended with non-millisecond utilities",
  "files": [
    "dist/**/*"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "prebuild": "rm -rf dist",
    "package": "npm pack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prepare": "husky"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@types/ms": "^2.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "ms": "^2.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjkid221/ms-extended.git"
  },
  "bugs": {
    "url": "https://github.com/mjkid221/ms-extended/issues"
  },
  "homepage": "https://github.com/mjkid221/ms-extended#readme"
}
